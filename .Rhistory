# 指定要搜索的字符串
search_term <- "check_linewidth"
# 获取当前目录及其子目录中的所有R脚本文件
files <- list.files(pattern = "\\.R$", recursive = TRUE)
# 遍历每个文件并查找包含搜索字符串的行
for (file in files) {
# 读取文件内容
lines <- readLines(file, warn = FALSE)
# 查找包含搜索字符串的行
match_lines <- grep(search_term, lines, value = TRUE)
# 如果找到匹配的行，打印文件名和匹配的行
if (length(match_lines) > 0) {
cat("在文件:", file, "\n")
cat("匹配的行:\n")
cat(match_lines, sep = "\n")
cat("\n\n")
}
}
